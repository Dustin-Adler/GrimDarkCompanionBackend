<div id="<%= dom_id weapon %>">
  <p>
    <strong>Id:</strong>
    <%= weapon.id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= weapon.name %>
  </p>

  <p>
    <strong>Meelee:</strong>
    <%= weapon.meelee %>
  </p>

  <p>
    <strong>Range:</strong>
    <%= weapon.range %>
  </p>

  <p>
    <strong>Attacks:</strong>
    <%= weapon.attacks %>
  </p>

  <p>
    <strong>Proficiency:</strong>
    <%= weapon.proficiency %>
  </p>

  <p>
    <strong>Strength:</strong>
    <%= weapon.strength %>
  </p>

  <p>
    <strong>Damage:</strong>
    <%= weapon.damage %>
  </p>

</div>

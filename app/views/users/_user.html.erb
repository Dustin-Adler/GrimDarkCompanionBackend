<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= user.password_digest %>
  </p>

  <p>
    <strong>Session token:</strong>
    <%= user.session_token %>
  </p>

  <p>
    <strong>Time zone:</strong>
    <%= user.time_zone %>
  </p>

</div>
